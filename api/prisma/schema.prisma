datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  id         String   @id
  title      String
  desc       String
  priority   Int
  status     String
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}
